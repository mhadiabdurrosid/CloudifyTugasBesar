:{$PORT:80} {

    # Enable FrankenPHP
    php_server

    # Root folder aplikasi (FIX: gunakan /app)
    root * /app

    # Direktori index
    file_server

    # Handle file PHP
    php *.php

    # Route otomatis ke index.php (jika file tidak ada)
    @notFile {
        not file
    }
    rewrite @notFile /index.php
}
